[module]
desc = Mail Report
exec = %%MODULES_DIR%%/mail_mod.py
files = /var/log/maillog[.#]
enabled = yes
internal = yes
outhtml = yes
priority = 5

[conf]
##
# Enable sendmail, postfix, or both
#
enable_sendmail = 1
enable_postfix = 0
##
# Report at most this many "top things"
#
top_report_limit = 5
